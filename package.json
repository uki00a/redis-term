  "author": "uki00a",
  "license": "MIT",
  "private": null,
  "engines": {
    "node": ">= 8"
  },
  "scripts": {
    "build": "babel src --out-dir lib --ignore './src/**/*.test.js'",
    "watch": "babel src --verbose --watch --out-dir lib --ignore './src/**/*.test.js'",
    "clean": "rm -rf ./lib",
    "test": "mocha -r @babel/register ./src/**/*.test.js",
    "integration-test": "mocha -r @babel/register './integration-test/**/*.test.js'",
    "test/esm": "mocha -r esm ./src/**/*.test.js"
  },
  "dependencies": {
    "ioredis": "^4.2.0",
    "neo-blessed": "^0.2.0",
