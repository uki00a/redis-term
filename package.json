    "react-blessed": "^0.5.0",
    "react-redux": "^6.0.1",
    "react-router": "^4.3.1",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0",
    "ssh2": "^0.8.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.5",
    "@babel/core": "^7.1.6",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-transform-modules-commonjs": "^7.1.0",
    "@babel/plugin-transform-react-jsx": "^7.1.6",
    "@babel/register": "^7.0.0",
    "eslint": "^5.15.0",
    "eslint-plugin-react": "^7.12.4",
    "esm": "^3.0.84",
    "mocha": "^5.2.0",
    "react-test-renderer": "^16.8.1"
